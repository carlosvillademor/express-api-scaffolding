{
  "name": "express-api-scaffolding",
  "version": "0.1.0",
  "description": "express-api-scaffolding",
  "homepage": "https://github.com/carlosvillademor/express-api-scaffolding",
  "author": {
    "name": "Carlos Fernandez Garcia",
    "email": "carlos.villademor@gmail.com"
  },
  "main": "backend/src/server/server.js",
  "keywords": [
    "express",
    "api",
    "scaffolding"
  ],
  "repository": "carlosvillademor/express-api-scaffolding",
  "devDependencies": {
    "gulp": "^3.6.0",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^1.0.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^0.9.0",
    "gulp-mocha": "^2.0.0",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "pre-git": "^1.2.11",
    "really-need": "^1.7.0",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "express": "^4.13.3"
  },
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp",
    "commit": "commit-wizard"
  },
  "license": "MIT",
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "commitizen": "node_modules/pre-git/node_modules/cz-conventional-changelog",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "prepublish",
        "npm test"
      ],
      "post-commit": [
        "git st"
      ],
      "post-merge": []
    }
  }
}
